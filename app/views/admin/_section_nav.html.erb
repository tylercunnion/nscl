<ul>
	<li>Members
		<ul>
			<li><%=link_to "New Member", new_member_url %></li>
			<% unless @member.blank? -%><li><%=link_to "Edit this member", edit_member_url(@member) %></li>
			<li><%= link_to "Delete this member", member_url(@member), :confirm => "Are you sure?", :method => :delete %> </li>
			<li><%=link_to "Register this member", {:controller => 'admin/members', :action => 'register', :id => @member} %></li><% end -%>
			<li>Find a member:<br />
			<%= text_field_with_auto_complete :member, :name, {:size => 20}, :skip_style => true %></li>
		</ul>
	</li>
	<li>Schools
		<ul>
			<li><%=link_to "New School", new_school_url %></li>
			<% unless @school.blank? -%><li><%= link_to "Edit this school", edit_school_url(@school) %></li>
			<li><%= link_to "Delete this school", school_url(@school), :confirm => "Are you sure?", :method => :delete %></li><% end -%>
			<li><%= link_to "List all schools", schools_url %></li>
		</ul>
	</li>	<li>Links
		<ul>
			<li><%=link_to "New Link", new_link_url %></li>
			<% unless @link.blank? -%><li><%= link_to "Edit this link", edit_link_url(@link) %></li>
			<li><%= link_to "Delete this link", link_url(@link), :confirm => "Are you sure?", :method => :delete %></li><% end -%>
			<li><%=link_to "List all links", links_url %></li>
		</ul>
	</li>
	<li>Publications
		<ul>
			<li><%= link_to "New Publication", :controller => 'admin/publications', :action => "new"%></li>
			<% unless @publication.blank? -%><li><%= link_to "Edit this publication", :controller => 'admin/publications', :action => 'edit', :id => @publication %></li>
			<li><%= link_to "Delete this publication", {:controller => 'admin/publications', :action => 'delete', :id => @school}, :confirm => "Are you sure?", :method => :post %></li><% end -%>
			<li><%= link_to "List all publications", :controller => 'admin/publications', :action => 'list' %></li>
		</ul>
	</li>
</ul>