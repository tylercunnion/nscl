<h2>National Links</h2>
<% @national_links.each do |link| -%>
	<p><%= link_to link.name, link.url %>
	<% if link.description != nil and link.description != "" -%>
	<br /><%= link.description %>
	<% end -%>
	</p>
<% end -%>
<h2>State Links</h2>
<% @state_links.each do |link| -%>
	<p><%= link_to link.name, link.url %>
	<% if link.description != nil and link.description != "" -%>
	<br /><%= link.description %>
	<% end -%>
	</p>
<% end -%>